      "end_time": 1,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "it does address the Missing and Murdered aboriginal woman, right"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
