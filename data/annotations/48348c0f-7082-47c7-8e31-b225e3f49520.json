      "start_time": 1,
      "end_time": 2,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "and I'm terribly, I feel terrible, terrible about that."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
